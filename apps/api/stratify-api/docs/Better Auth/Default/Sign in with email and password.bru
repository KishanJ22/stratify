meta {
  name: Sign in with email and password
  type: http
  seq: 5
}

post {
  url: {{baseUrl}}/sign-in/email
  body: json
  auth: none
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{password}}",
    "callbackURL": "",
    "rememberMe": false
  }
}

script:post-response {
  const bearerToken = res.headers["set-auth-token"];
  bru.setEnvVar("bearerToken", bearerToken);
}
